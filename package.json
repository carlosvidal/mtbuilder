{
  "name": "page-builder",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && NODE_ENV=production vite build",
    "build:dev": "rimraf dist && vite build --mode development",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "analyze": "vite build --mode analyze",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.39.2",
    "jsdom": "^28.0.0",
    "rimraf": "^5.0.5",
    "terser": "^5.24.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
